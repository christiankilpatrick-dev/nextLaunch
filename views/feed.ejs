<%- include('partials/header') -%>
  <div class="container">
    <div class="row justify-content-center mt-5">
      <% for(var i=0; i<launches.length; i++) {%>
          <article class="post">
            <header>
              <div class="title">
                <h2><a href="single.html"><%= launches[i].mission.name%></a></h2>
              </div>
              <div class="meta">
                
                <a href="#" class="author"><span class="name"><%= launches[i].launch_service_provider.name%></span></a>
              </div>
            </header>
            
            <div class="postBody">
              <div class='postImage'>
                <img id="rockets" src="<%= launches[i].image%>" alt="" /></a>
              </div>
              <div class = 'postInfo'>
                <time class="published" datetime="2015-11-01"><%= launches[i].window_start%></time>
                <p><%= launches[i].pad.location.name%></p>
                <span ><%= launches[i].status.name%></span>
                <p ><%= launches[i].mission.description%></p>
              </div>
            </div>
            <footer>
              <ul class="actions">
                <li><a href="/post/<%= launches[i].id%>" class="button large">Continue Reading</a></li>
              </ul>
              <ul class="stats">
                <li><a href="#">General</a></li>
                <li><a href="#" class="icon solid fa-heart">28</a></li>
                <li><a href="#" class="icon solid fa-comment">128</a></li>
              </ul>
            </footer>
          </article>
      <% } %>
    </div>
  </div>  
<%- include('partials/footer') -%>