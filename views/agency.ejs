<%- include('partials/header') -%>
<div class="container">
	<div class="row justify-content-center mt-5">
		<div class="col-6">
			<h2><%= agency.name %></h2>
			<div class="imageContainer">
				<img id="rockets" class="" src="<%= agency.logo_url%>" />
			</div>
		</div>
		<div class="col-3 mt-5">
			<p><%= agency.abbrev%></p>
			<p><%= agency.country_code%></p>
			<p>Successful Launches: <%= agency.successful_launches%></p>
			<ul class="actions">
				<li class="continue">
					<a href="<%= agency.info_url%>" class="button large"
						>Continue Reading</a
					>
				</li>
			</ul>
		</div>

		<div class="col-6 mt-5">
			<a class="btn btn-primary" href="/">Return to Feed</a>
		</div>
	</div>
</div>

<%- include('partials/footer') -%>
